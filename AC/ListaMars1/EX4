li $t0, 8	#x = 8
li $t1, 4	#y = 4
li $t2, 2       #i = 2

while: 
	addi $t1, $t1, 2    # y = y + 2
	subi $t2, $t2, 1    # i = i - 1
	beq $t2, 0, continuar # verifica se i > 0, i >= 1 Ã© a mesma coisa que i > 0. das duas maneiras este while vai rodar apenas uma vez
	j while
	
continuar:
	li $t5, 0
	add $t5, $t0, $t1  #esta declarando e somando o valor de x e y em z